{
  "name": "Prometheus Stack",
  "version": "1.10.0",
  "slug": "prometheus-stack",
  "description": "Prometheus, Alertmanager and Karma in one add-on",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 8080,
  "panel_icon": "mdi:chart-timeline",
  "build": {
    "dockerfile": "Dockerfile",
    "args": []
  },
  "ports": {
    "9090/tcp": 9090,
    "9093/tcp": 9093,
    "9115/tcp": 9115,
    "8080/tcp": null
  },
  "ports_description": {
    "9090/tcp": "Prometheus",
    "9093/tcp": "Alertmanager",
    "9115/tcp": "Blackbox Exporter"
  },
  "webui": "http://[HOST]:[PORT:9090]",
  "host_network": false,
  "hassio_api": true,
  "hassio_role": "admin",
  "options": {
    "alertmanager_receiver": "default",
    "alertmanager_to_email": "example@example.com",
    "home_assistant_url": "http://supervisor/core",
    "home_assistant_token": "",
    "blackbox_targets": [
      {
        "name": "Home Assistant",
        "url": "http://supervisor/core"
      }
    ]
  },
  "schema": {
    "alertmanager_receiver": "str",
    "alertmanager_to_email": "email",
    "home_assistant_url": "str",
    "home_assistant_token": "str",
    "blackbox_targets": [
      {
        "name": "str",
        "url": "str"
      }
    ]
  }
}
