{
  "name": "Prometheus Stack",
  "version": "0.1",
  "slug": "prometheus_stack",
  "description": "Prometheus, Alertmanager and Karma in one add-on",
  "arch": ["aarch64", "amd64", "armv7"],
  "startup": "services",
  "boot": "auto",
  "ingress": true,
  "ingress_port": 8080,
  "panel_icon": "mdi:chart-timeline",
  "ports": {
    "9090/tcp": 9090,
    "9093/tcp": 9093,
    "8080/tcp": null
  },
  "ports_description": {
    "9090/tcp": "Prometheus",
    "9093/tcp": "Alertmanager"
  },
  "webui": "http://[HOST]:[PORT:9090]",
  "options": {
    "alertmanager_receiver": "default",
    "alertmanager_to_email": "example@example.com"
  },
  "schema": {
    "alertmanager_receiver": "str",
    "alertmanager_to_email": "email"
  }
}
